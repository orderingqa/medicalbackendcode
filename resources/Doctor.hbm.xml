<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="ls.jtsk.model.Doctor" table="doctor">
	<id name="doctorId" column="doctorId" type="long">
		<generator class="native"></generator>
	</id>
	<property name="doctorName" column="doctorName" type="string"></property>
</class>
</hibernate-mapping>